@model IEnumerable<Mcd.HospitalManagement.Web.Models.WardModel>


<h2>Ward Details</h2>
<div class="row">
    <div class="col-xs-3">
        <ol class="breadcrumb">
            <li><a href="~/AdminHome/Index">Home</a></li>
            @*<li><a href="#">Library</a></li>*@
            <li class="active">Ward Details</li>
        </ol>
    </div>
</div>
<div class="col-xs-1">
    <ul class="nav nav-pills" role="tablist">     

        <li role="presentation" class="active"><a href="~/Ward/Index">Wards</a></li>
        <li role="presentation" style="width: 150px"><a href="~/Ward/Create">Add New Ward</a></li>

    </ul>
</div>
<div class="col-xs-1"></div>
<div class="col-xs-10">
    <table class="table table-condensed">
        <tr class="warning">
            <th>
                @Html.DisplayNameFor(model => model.WardNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WardFee)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr class="warning">
                <td>
                    @Html.DisplayFor(modelItem => item.WardNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WardFee)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
</div>